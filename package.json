{
  "name": "mercy",
  "version": "0.0.1",
  "description": "Flow control library",
  "keywords": [
    "flow",
    "control",
    "test",
    "helper"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rm -rf node_modules",
    "test": "node_modules/.bin/lab -m 90000",
    "test-cov": "node_modules/.bin/lab -t 100 -m 90000 -v -p",
    "test-cov-html": "node_modules/.bin/lab -r html -o coverage.html -m 90000 -p",
    "test-cov-junit": "node_modules/.bin/lab -r junit -o coverage.xml -m 90000 -p"
  },
  "engines": {
    "node": ">=8.x.x",
    "npm": ">=5.x.x"
  },
  "dependencies": {
    "async": "2.x.x",
    "glue": "4.x.x",
    "hoek": "4.x.x",
    "insync": "2.x.x",
    "joi": "13.x.x",
    "nock": "9.x.x"
  },
  "devDependencies": {
    "code": "4.x.x",
    "lab": "13.x.x"
  }
}
